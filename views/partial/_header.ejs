<header>
    <nav class="navbar navbar-expand-md navbar-dark bg-info">
      <a class="navbar-brand" href="/urls" style="font-size: 28px; font-family: 'Righteous', cursive">TinyApp</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-item nav-link" style="font-family: 'Righteous', cursive" href="/urls">My URLs</a>
          <a class="nav-item nav-link" style="font-family: 'Righteous', cursive" href="/urls/new">Create New URL</a>
        </div>
      </div>
      <% if (user) {%> 
        <h6 class="nav-item nav-link" style="margin-bottom: 4px; font-size: 17px; font-family: 'Didact Gothic', sans-serif;">Logged in as: <%= user.email %></h6>
          <form class="form-inline" method="POST" action="/logout" style="margin-bottom: 8px">
           <button type="submit" class="btn btn-outline-dark">Logout</button>
        </form>
      <% } else {%> 
        <div class="navbar-nav">
          <a class="nav-item nav-link" style="font-family: 'Righteous', cursive; font-size: 18px" href="/login">Login</a>
          <a class="nav-item nav-link" style="font-family: 'Righteous', cursive; font-size: 18px" href="/Register">Register</a>
        </div>
      <% } %>
    </nav>
  </header>